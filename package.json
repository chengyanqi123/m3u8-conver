{
    "packageManager": "pnpm@8.14.1",
    "name": "m3u8-conver",
    "description": "Convert network or local m3u8 files into media files.将网络或者本地的m3u8文件转换成媒体文件",
    "version": "1.0.2",
    "type": "module",
    "main": "./dist/index.js",
    "engines": {
        "node": ">=16"
    },
    "bin": {
        "mconver": "./dist/cli.js"
    },
    "exports": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
    },
    "scripts": {
        "pack": "tsc -w",
        "test": "node dist/index.js",
        "test:ts": "ts-node src/index.js"
    },
    "keywords": [
        "m3u8",
        "m3u8-conver",
        "m3u8-parser",
        "m3u8-download",
        "m3u8-downloader"
    ],
    "author": {
        "name": "Cheng Yanqi",
        "email": "erickcheng@163.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chengyanqi123/m3u8-conver"
    },
    "homepage": "https://github.com/chengyanqi123",
    "license": "MIT",
    "dependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "chalk": "^5.3.0",
        "cli-progress": "^3.12.0",
        "commander": "^12.0.0",
        "got": "^14.0.0",
        "m3u8-parser": "^7.1.0",
        "p-queue": "^8.0.1",
        "progress": "^2.0.3"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.11.5",
        "@types/got": "^9.6.12",
        "@types/node": "^20.11.5",
        "@types/progress": "^2.0.7",
        "typescript": "^5.3.3"
    }
}